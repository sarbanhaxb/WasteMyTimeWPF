                                </Canvas>
                            </Canvas>
                        </Canvas>
                    </Canvas>
                </Viewbox>
                <Button.Resources>
                    <Style TargetType="Border">
                        <Setter Property="CornerRadius" Value="15"/>
                    </Style>
                </Button.Resources>
            </Button>
